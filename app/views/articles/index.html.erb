<div class = "container article-list">
<div class = "row article-list" >
<div class = "col-ms-12 article-list_article-index">
<table class= "user-list_user-table table-hover table-inverse">
 <% @articles.each do |article| %>
 <thead></thead>
 	  <tbody>
         <tr>
            <td><%=link_to user_path(article.user) do%><%= attachment_image_tag article.user, :self_image, :fill, 60, 60, format: 'jpeg', class: "img pull-left self-img", fallback: "no_image.jpg" %><% end %></td>
            <td><%=link_to article_path(article.id) do%><%= article.title %><% end %></td>
            <td><%= article.body %></td>
      	</tr>
      </tbody>
          <% end %>

  </table>
</div>
</div>
</div>